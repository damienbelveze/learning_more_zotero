@article{al-niaeemiESTIMATIONHEAVYMETALS2020,
  title = {{{ESTIMATION OF SOME HEAVY METALS BIOACCUMULATION IN TISSUES OF THE INTESTINAL CESTODA POSTGANGESIA ARMATA AND ITS DEFINITIVE HOST}}; {{THE EUROPEAN CATFISH SILURUS GLANIS}} ({{L}}.) {{FROM TIGRIS RIVER PASSING THROUGH NORTHERN N}}. {{GOVERNORATE}}, {{IRAQ}}},
  author = {family=Al-Niaeemi, given=BH, given-i=BH and family=Dawood, given=MH, given-i=MH and family=Al-Kallak, given=SN, given-i=SN and family=Ibrahim, given=RH, given-i=RH},
